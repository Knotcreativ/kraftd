{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": []
    },
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/dashboard/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/terms.html",
      "rewrite": "/terms.html"
    },
    {
      "route": "/privacy.html",
      "rewrite": "/privacy.html"
    },
    {
      "route": "/signin.html",
      "rewrite": "/signin.html"
    },
    {
      "route": "/signup.html",
      "rewrite": "/signup.html"
    },
    {
      "route": "/forgot-password.html",
      "rewrite": "/forgot-password.html"
    },
    {
      "route": "/reset-password.html",
      "rewrite": "/reset-password.html"
    },
    {
      "route": "/verify-email.html",
      "rewrite": "/verify-email.html"
    },
    {
      "route": "/chat.html",
      "rewrite": "/chat.html"
    },
    {
      "route": "/landing.html",
      "rewrite": "/landing.html"
    },
    {
      "route": "/",
      "rewrite": "/landing.html"
    },
    {
      "route": "/*",
      "rewrite": "/landing.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,sass,less}",
      "*.{jpg,svg,gif,ico,png,webp}",
      "*.{js,jsx,ts,tsx}",
      "*.{woff,woff2}"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/signin.html",
      "statusCode": 302
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  },
  "globalHeaders": [
    {
      "name": "Cache-Control",
      "value": "no-cache, no-store, must-revalidate"
    },
    {
      "name": "X-Content-Type-Options",
      "value": "nosniff"
    },
    {
      "name": "X-Frame-Options",
      "value": "DENY"
    }
  ]
}

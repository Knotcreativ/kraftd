{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/*.{json,jpg,gif,png,webp,css,js,svg,eot,otf,ttf,woff,woff2}"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "routes": [
    {
      "route": "/chat",
      "rewrite": "/chat.html"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated", "anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "apiVersion": "1.0.0"
}

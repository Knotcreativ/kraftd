{
  "routes": [
    {
      "route": "/api/*"
    },
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/dashboard/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{css,scss,sass,less}",
      "*.{jpg,svg,gif,ico,png,webp}",
      "*.{js,jsx,ts,tsx}",
      "*.{woff,woff2}"
    ]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/index.html",
      "statusCode": 302
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".wasm": "application/wasm"
  },
  "globalHeaders": [
    {
      "name": "Cache-Control",
      "value": "no-cache, no-store, must-revalidate"
    },
    {
      "name": "X-Content-Type-Options",
      "value": "nosniff"
    },
    {
      "name": "X-Frame-Options",
      "value": "DENY"
    }
  ]
}

{
  "routes": [
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/dashboard/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}
